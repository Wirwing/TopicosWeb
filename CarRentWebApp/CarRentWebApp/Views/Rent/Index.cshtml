@{
    ViewBag.Title = "Home Page";
}


@model System.Collections.Generic.List<CarRentWebApp.Models.Rent>

<div class="row">
    <div class="col-md-12">
        <table class="table">
            <tr>
                <th>Nombre</th>
                <th>Proveedor</th>
                <th>Operaciones</th>
            </tr>
            @foreach (var item in Model) {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.Name</td>
                    <td>@item.Provider</td>
                    <td>
                        @Html.ActionLink("Rentar", "RentCar", new { id = item.Id, provider = item.Provider })
                    </td>
                </tr>
            }
        </table>
    </div>
</div>