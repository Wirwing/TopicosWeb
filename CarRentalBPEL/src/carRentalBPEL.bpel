                <from>string()</from>
                <to variable="KutzGetCarsIn" part="parameters"/>
            </copy>
        </assign>
        <invoke name="KutzCarsInvoke" partnerLink="KutzRentalPartnerLink" operation="getCars" xmlns:tns="http://WS.KutzCarRental/" portType="tns:KutzCarRentalWS" inputVariable="KutzGetCarsIn" outputVariable="KutzGetCarsOut"/>
