            <copy>
                <from variable="KutzGetCarsOut" part="parameters"/>
                <to variable="InitOperationOut" part="kutzCars"/>
            </copy>
        </assign>
        <reply name="InitReply" partnerLink="CarRentalBPELPartnerLink" operation="InitOperation" xmlns:tns="http://j2ee.netbeans.org/wsdl/CarRentalBPEL/src/CarRentalBPEL" portType="tns:CarRentalBPELPortType" variable="InitOperationOut"/>
